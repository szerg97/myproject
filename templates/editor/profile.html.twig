{% extends "base.html.twig" %}

{% block title %}
    MY EDITOR
{% endblock %}

{% block body %}
    {% if app.session.get('userName') is not empty %}
        {% if form is not null and form is not empty %}
            {% form_theme form 'form_table_layout.html.twig' %}
            {{ form_start(form) }}
            {{ form_widget(form) }}
            {{ form_end(form) }}
        {% endif %}
    {% endif %}
{% endblock %}